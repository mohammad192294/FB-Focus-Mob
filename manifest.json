{
  "manifest_version": 3,
  "name": "FB Project Zero (Final)",
  "version": "10.0",
  "description": "The absolute final solution to remove all video content, stories, and gaps from Facebook Mobile.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://*.facebook.com/*",
    "*://*.fb.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*",
        "*://*.fb.com/*"
      ],
      "css": ["styles.css"],
      "js": ["content_script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
